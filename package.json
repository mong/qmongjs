{
  "name": "qmongjs",
  "version": "0.7.1",
  "description": "javascript module for qmongr",
  "private": true,
  "type": "module",
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "is-ci \"test:coverage\" \"test:base\"",
    "test:base": "react-scripts test",
    "test:once": "npm run test:base -- --watchAll=false",
    "test:coverage": "npm run test:once -- --coverage",
    "test:debug": "react-scripts --inspect-brk test --runInBand --no-cache",
    "test:staged": "npm run test:once -- --findRelatedTests",
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "lint": "eslint --ignore-path .gitignore --ext js,ts,tsx src",
    "prettier": "prettier \"**/*.+(js|jsx|ts|tsx|json|html|yml|yaml|css|md)\"",
    "format": "npm run prettier -- --write",
    "check-format": "npm run prettier -- --list-different",
    "check-types": "tsc --noEmit",
    "validate": "npm-run-all --parallel check-types check-format lint test:once build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mong/qmongjs.git"
  },
  "license": "GPL-3.0-only",
  "bugs": {
    "url": "https://github.com/mong/qmongjs/issues"
  },
  "dependencies": {
    "@testing-library/jest-dom": "^5.11.6",
    "@testing-library/react": "^11.2.0",
    "@testing-library/user-event": "^12.2.2",
    "@types/d3": "^6.2.0",
    "@types/jest": "^26.0.15",
    "@types/node": "^14.14.9",
    "@types/react": "^16.9.56",
    "@types/react-dom": "^16.9.9",
    "@types/react-select": "^3.0.26",
    "d3": "^6.1.1",
    "is-ci-cli": "^2.1.2",
    "prettier": "^2.1.2",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-scripts": "^4.0.0",
    "react-select": "^3.1.0",
    "resize-observer-polyfill": "^1.5.1",
    "source-map-explorer": "^2.5.0",
    "typescript": "^4.0.5"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "husky": "^4.3.0",
    "lint-staged": "^10.5.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.2.0"
  },
  "jest": {
    "coverageThreshold": {
      "global": {
        "statements": 3,
        "branches": 4,
        "functions": 1,
        "lines": 3
      }
    }
  }
}
