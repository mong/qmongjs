{
  "name": "qmongjs",
  "homepage": "https://www.skde.no/kvalitetsregistre",
  "description": "The kvalitetsregistre page of www.skde.no",
  "private": true,
  "scripts": {
    "postinstall": "husky install",
    "start": "next dev",
    "build": "next build",
    "serve": "next start",
    "test": "is-ci test:coverage test:base",
    "test:base": "jest test",
    "test:once": "yarn run test:base --watchAll=false",
    "test:once:watch": "yarn run test:base --watchAll=true",
    "test:coverage": "yarn run test:once --coverage",
    "test:debug": "react-scripts --inspect-brk test --runInBand --no-cache",
    "test:staged": "yarn run test:once --findRelatedTests",
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "lint": "eslint --ignore-path .gitignore --ext js,ts,tsx src",
    "prettier": "prettier \"**/*.+(js|jsx|ts|tsx|json|html|yml|yaml|css|md)\"",
    "format": "yarn run prettier --write",
    "check-format": "yarn run prettier --check",
    "check-types": "tsc --noEmit",
    "validate": "yarn run check-types && yarn run check-format && yarn run lint && yarn run test:once && yarn run build",
    "storybook": "start-storybook -p 6006 -s public",
    "build-storybook": "build-storybook -s public",
    "build-doc": "typedoc src/index.tsx src/*/*/index.[jt]s* src/*/*/*/index.[jt]s*"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mong/qmongjs.git"
  },
  "license": "GPL-3.0-only",
  "bugs": {
    "url": "https://github.com/mong/qmongjs/issues"
  },
  "dependencies": {
    "@emotion/react": "11.10.4",
    "@emotion/styled": "11.10.4",
    "@mui/icons-material": "5.10.6",
    "@mui/material": "5.10.6",
    "@sentry/nextjs": "^7.13.0",
    "@sentry/tracing": "7.13.0",
    "@visx/event": "2.6.0",
    "@visx/tooltip": "2.10.0",
    "d3": "7.6.1",
    "d3-format": "3.1.0",
    "next": "^12.3.1",
    "next-query-params": "^4.0.0",
    "query-string": "7.1.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-icons": "4.4.0",
    "react-markdown": "8.0.3",
    "react-query": "3.39.2",
    "react-select": "5.4.0",
    "remark-gfm": "3.0.1",
    "resize-observer-polyfill": "1.5.1",
    "source-map-explorer": "2.5.2",
    "use-query-params": "^2.1.1"
  },
  "eslintConfig": {
    "extends": [
      "next"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "13.4.0",
    "@testing-library/user-event": "14.4.3",
    "@types/d3": "7.4.0",
    "@types/jest": "29.0.3",
    "@types/node": "18.0.0",
    "@types/react": "18.0.20",
    "@types/react-dom": "18.0.6",
    "eslint": "8.23.1",
    "eslint-config-next": "^12.3.1",
    "husky": "8.0.1",
    "is-ci-cli": "2.2.0",
    "jest": "^29.0.3",
    "jest-environment-jsdom": "^29.0.3",
    "lint-staged": "13.0.3",
    "msw": "0.47.3",
    "next-router-mock": "^0.7.4",
    "prettier": "2.7.1",
    "typedoc": "0.23.15",
    "typescript": "4.8.3"
  },
  "packageManager": "yarn@3.2.3"
}
