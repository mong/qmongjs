{
  "name": "qmongjs",
  "homepage": "https://d13kv0v5vl0wju.cloudfront.net/kvalitetsregistre",
  "version": "2.0.5",
  "description": "The skde.org (to be skde.no) web page",
  "private": true,
  "scripts": {
    "postinstall": "husky install",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "is-ci test:coverage test:base",
    "test:base": "react-scripts test",
    "test:once": "yarn run test:base -- --watchAll=false",
    "test:coverage": "yarn run test:once -- --coverage",
    "test:debug": "react-scripts --inspect-brk test --runInBand --no-cache",
    "test:staged": "yarn run test:once -- --findRelatedTests",
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "lint": "eslint --ignore-path .gitignore --ext js,ts,tsx src",
    "prettier": "prettier \"**/*.+(js|jsx|ts|tsx|json|html|yml|yaml|css|md)\"",
    "format": "yarn run prettier --write",
    "check-format": "yarn run prettier --check",
    "check-types": "tsc --noEmit",
    "validate": "yarn run check-types && yarn run check-format && yarn run lint && yarn run test:once && yarn run build",
    "storybook": "start-storybook -p 6006 -s public",
    "build-storybook": "build-storybook -s public",
    "build-doc": "typedoc src/index.tsx src/*/*/index.[jt]s* src/*/*/*/index.[jt]s*"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mong/qmongjs.git"
  },
  "license": "GPL-3.0-only",
  "bugs": {
    "url": "https://github.com/mong/qmongjs/issues"
  },
  "dependencies": {
    "@sentry/react": "^6.3.6",
    "@sentry/tracing": "^6.3.6",
    "d3": "^6.7.0",
    "query-string": "^7.0.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.2",
    "react-query": "^3.16.0",
    "react-router-dom": "^5.2.0",
    "react-scripts": "^4.0.3",
    "react-select": "^4.3.1",
    "resize-observer-polyfill": "^1.5.1",
    "source-map-explorer": "^2.5.2",
    "use-query-params": "^1.2.2"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.14.3",
    "@babel/preset-env": "^7.14.4",
    "@babel/preset-typescript": "^7.13.0",
    "@storybook/addon-actions": "^6.2.9",
    "@storybook/addon-essentials": "^6.1.21",
    "@storybook/addon-links": "^6.2.9",
    "@storybook/node-logger": "^6.2.9",
    "@storybook/preset-create-react-app": "^3.1.7",
    "@storybook/react": "^6.1.21",
    "@testing-library/jest-dom": "^5.12.0",
    "@testing-library/react": "^11.2.7",
    "@testing-library/user-event": "^13.1.9",
    "@types/d3": "^6.3.0",
    "@types/faker": "^5.5.5",
    "@types/jest": "^26.0.23",
    "@types/node": "^15.3.0",
    "@types/react": "^17.0.5",
    "@types/react-dom": "^17.0.5",
    "@types/react-router-dom": "^5.1.7",
    "@types/react-select": "^4.0.15",
    "@types/testing-library__jest-dom": "^5.9.5",
    "@typescript-eslint/eslint-plugin": "^4.22.0",
    "babel-jest": "^27.0.2",
    "codecov": "^3.8.2",
    "eslint": "^7.24.0",
    "eslint-config-react-app": "^6.0.0",
    "eslint-plugin-flowtype": "^5.7.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.3.5",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.23.2",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-testing-library": "^4.1.1",
    "faker": "^5.5.3",
    "husky": "^6.0.0",
    "is-ci-cli": "^2.2.0",
    "jest": "^27.0.4",
    "lint-staged": "^11.0.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.2.1",
    "ts-jest": "^27.0.3",
    "typedoc": "^0.20.36",
    "typescript": "^4.2.4"
  },
  "jest": {
    "coverageThreshold": {
      "global": {
        "statements": 3,
        "branches": 4,
        "functions": 1,
        "lines": 3
      }
    }
  }
}
